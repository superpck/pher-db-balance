{
  "name": "db-balance2",
  "version": "1.0.0",
  "description": "Node.js Cron Job application for syncing data from master to offline server",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "migrate:master": "knex migrate:latest --knexfile src/config/knexfile.js --env master",
    "migrate:offline": "knex migrate:latest --knexfile src/config/knexfile.js --env offline"
  },
  "keywords": [
    "express",
    "knex",
    "mysql",
    "database-sync"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.6.2",
    "dayjs": "^1.11.19",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "knex": "^3.1.0",
    "mysql2": "^3.6.5",
    "node-cron": "^3.0.3"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  }
}
